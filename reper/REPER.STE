REPORT.BASED.ON.PANEL:1
NAME:~Generate base STE (without reports)~
REPORT.MODE:TEXT
OUTPUT.FILENAME:~BASE.STE~
CREATE.DISK.FILE
REPORT.INDEX: 1
TEXT.LINES=0 TOP.MARGIN=0 BOTTOM.MARGIN=0 LEFT.MARGIN=0
REPORT.FORM:
FIRST.PAGE.HEADER
SET.RV:11 FORMULA:~1~
FORMULA.END;
OTHER.PAGE.HEADER
TWO.LEVEL.REPORT.HEADER
~rv11 means we have not yet seen REPORTS:~  CR
REPORT.BODY
SET.RV:1 FORMULA: FIELD=P1F2 ~="REPORTS:"~
FORMULA.END;
SET.RV:11 FORMULA:~rv11 AND (NOT rv1)~
FORMULA.END;
 FIELD=P1F2;~A78;;T~ ~ ~
STOP.REPORT.ON.RV:11
TWO.LEVEL.FOOTER
PAGE.FOOTER
FINAL.FOOTER
REPORT.END;

REPORT.BASED.ON.PANEL:2
NAME:~Generate individual report STE files~
REPORT.MODE:TEXT
REPORT.INDEX: 1
TEXT.LINES=0 TOP.MARGIN=0 BOTTOM.MARGIN=0 LEFT.MARGIN=0
REPORT.FORM:
FIRST.PAGE.HEADER
OTHER.PAGE.HEADER
TWO.LEVEL.REPORT.HEADER
REPORT.BODY
SET.RV:100 FORMULA:~apply.format['GZZ9;;B';~  FIELD=P2F3 ~] '.STE'~
FORMULA.END;
TURN.FILE.OFF
BEGIN.WRITING.TO.FILENAME.IN.RV:100
SET.RV:30 FORMULA: FIELD=P2F1
FORMULA.END;
SET.RV:31 FORMULA: FIELD=P2F2
FORMULA.END;
~REPORTS:~  CR
VIRTUAL.SUBREPORT.TO.PANEL:1 TO.INDEX:1 FIELD.LIST:0
FIRST.PAGE.HEADER
OTHER.PAGE.HEADER
TWO.LEVEL.REPORT.HEADER
REPORT.BODY
SET.RV:11 FORMULA:~rv30 <= ~  FIELD=P1F1 ~ and ~  FIELD=P1F1 ~ <= rv31~
FORMULA.END;
SKIP.RECORD.ON.RV:11
 FIELD=P1F2;~A78;;T~ ~ ~
TWO.LEVEL.FOOTER
PAGE.FOOTER
FINAL.FOOTER
END.SUBREPORT.FOR.PANEL:1
TWO.LEVEL.FOOTER
PAGE.FOOTER
FINAL.FOOTER
REPORT.END;

