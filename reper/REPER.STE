REPORTS:
REPORT.BASED.ON.PANEL:1
NAME:~Index the reports~
REPORT.MODE:TEXT
REPORT.INDEX: 1
TEXT.LINES=0 TOP.MARGIN=0 BOTTOM.MARGIN=0 LEFT.MARGIN=0
REPORT.FORM:
FIRST.PAGE.HEADER
SET.RV:10 FORMULA:~0~
FORMULA.END;
SET.RV:11 FORMULA:~0~
FORMULA.END;
OTHER.PAGE.HEADER
TWO.LEVEL.REPORT.HEADER
~rv10 is report number~  CR
~rv11 means we have seen REPORTS:~  CR
~rv1 means we are seeing REPORTS:~  CR
~rv2 means we are seeing NAME:~  CR
~rv20 is the report name~  CR
~rv30 is report first line number~  CR
~rv31 is report last line number~
REPORT.BODY
SET.RV:1 FORMULA: FIELD=P1F2 ~="REPORTS:"~
FORMULA.END;
SET.RV:11 FORMULA:~rv11 or rv1~
FORMULA.END;
SKIP.RECORD.ON.RV:11
SET.RV:31 FORMULA: FIELD=P1F1
FORMULA.END;
SET.RV:2 FORMULA: FIELD=P1F2 ~="NAME:"~
FORMULA.END;
SKIP.RECORD.ON.RV:2
VIRTUAL.SUBREPORT.TO.PANEL:1 TO.INDEX:1 FIELD.LIST:1
FIRST.PAGE.HEADER
OTHER.PAGE.HEADER
TWO.LEVEL.REPORT.HEADER
REPORT.BODY
SKIP.RECORD.ON.RV:10
SET.RV:31 FORMULA: FIELD=P1F1 ~-2~
FORMULA.END;
~Report ~ RV:10;~GZZ9;;B~ ~ ~ RV:30;~GZZZZ9;;B~ ~:~ RV:31;~GZZZZ9;;B~ ~ ~
RV:20;~A71;;T~
CREATE.RECORD.THRU.DOOR:0 TO.PANEL:2
REPORT.BODY
STORE.RV: 10 IN.FIELD:3
STORE.RV: 30 IN.FIELD:1
STORE.RV: 31 IN.FIELD:2
STORE.RV: 20 IN.FIELD:4
SAVE.RECORD.IN.PANEL:2
TWO.LEVEL.FOOTER
PAGE.FOOTER
FINAL.FOOTER
END.SUBREPORT.FOR.PANEL:1
SET.RV:10 FORMULA:~rv10+1~
FORMULA.END;
SET.RV:30 FORMULA: FIELD=P1F1 ~-1~
FORMULA.END;
SET.RV:20 FORMULA:~subfield[~  FIELD=P1F2 ~;'~~';2]~
FORMULA.END;
TWO.LEVEL.FOOTER
PAGE.FOOTER
FINAL.FOOTER
~Report ~ RV:10;~GZZ9;;B~ ~ ~ RV:30;~GZZZZ9;;B~ ~:~ RV:31;~GZZZZ9;;B~ ~ ~
RV:20;~A71;;T~
CREATE.RECORD.THRU.DOOR:0 TO.PANEL:2
REPORT.BODY
STORE.RV: 10 IN.FIELD:3
STORE.RV: 30 IN.FIELD:1
STORE.RV: 31 IN.FIELD:2
STORE.RV: 20 IN.FIELD:4
SAVE.RECORD.IN.PANEL:2
REPORT.END;

